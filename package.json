{
  "name": "ionic-app-base",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepush": "npm run lint",
    "lint": "ionic-app-scripts lint",
    "commitmsg": "node node_modules/commitplease"
  },
  "config": {
    "ionic_bundler": "rollup",
    "ionic_copy": "./scripts/app/copy.config.js",
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/compiler-cli": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@ionic/storage": "1.1.6",
    "intl": "1.2.5",
    "ionic-angular": "2.0.0-rc.4",
    "ionic-native": "2.2.12",
    "ionicons": "3.0.0",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26",
    "angular2-text-mask": "2.1.0",
    "@ramonornela/configuration": "nightly",
    "@ramonornela/url-resolver": "nightly",
    "@ramonornela/http": "nightly",
    "@ramonornela/http-plugins-ionic": "nightly"
  },
  "devDependencies": {
    "@ionic/app-scripts": "0.0.46",
    "typescript": "2.0.6",
    "tslint-ionic-rules": "0.0.8",
    "codelyzer": "2.0.0-beta.4",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "conventional-changelog-cli": "^1.2.0",
    "commitplease": "2.6.1",
    "husky": "^0.11.9"
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat", "fix", "docs", "style", "refactor", "perf", "test", "chore"
    ],
    "scope": "\\S+.*"
  }
}
