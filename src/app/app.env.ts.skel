import { OpaqueToken } from '@angular/core';

export const Env: string = 'dev';
export const EnvToken = new OpaqueToken('ENVTOKEN');
export const ProviderEnv = { provide: EnvToken, useValue: Env };

export const DeviceToken =  new OpaqueToken('DEVICETOKEN');
export const DeviceInfo = { provide: DeviceToken, useValue: {
    version: 'replace-version',
    bundle: 'replace-bundle'
  }
};
